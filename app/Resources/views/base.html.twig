<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <title>{% block title %}Bienvenido{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/appfrontend/bootstrap/css/bootstrap.css') }}">
            <!--link rel="stylesheet" href="{{ asset('bundles/appfrontend/font-awesome/css/font-awesome.css') }}"-->
            <!--link rel="stylesheet" href="{{ asset('bundles/appfrontend/foundation/css/foundation.css') }}"-->
            
            <link rel="stylesheet" href="{{ asset('bundles/appfrontend/css/animate.min.css') }}" type="text/css">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            {{ getTokens() }}
            
            {% include 'AppBackendBundle:Default:flash.message.html.twig' %}
        {% endblock %}
        {% block javascripts %}
            <script src="{{  asset('bundles/appfrontend/js/jquery-2.1.4.min.js') }}" ></script>
            <script src="{{  asset('bundles/appfrontend/bootstrap/js/bootstrap.js') }}" ></script>
            <!--script src="{{  asset('bundles/appfrontend/foundation/js/foundation.js') }}" ></script-->
            
            <script src="{{  asset('bundles/appfrontend/js/app.js') }}" ></script>
        {% endblock %}
    </body>
</html>
